{
  "name": "crypto",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "raz-environnement": "npx ts-node ./scripts/razenv.ts",
    "set-environnement": "npx ts-node ./scripts/setenv.ts",
    "start": "npm run set-environnement && ng serve --open=true --sourceMap=true",
    "build": "npm run set-environnement && ng build",
    "build-prod": "npm run set-environnement && ng build --prod --baseHref=./",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run raz-environnement"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@fortawesome/fontawesome-free": "~5.6.3",
    "@types/chart.js": "^2.7.40",
    "angular-bootstrap-md": "^10.1.1",
    "animate.css": "~3.7.2",
    "chart.js": "^2.7.2",
    "crypto-js": "^4.0.0",
    "hammerjs": "~2.0.8",
    "jssha": "^3.2.0",
    "mdb-ui-kit": "^3.6.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "^12.1.3",
    "@angular/compiler-cli": "~10.2.0",
    "@angular/localize": "^10.2.5",
    "@types/crypto-js": "^4.0.1",
    "@types/node": "^12.20.13",
    "@types/yargs": "^17.0.0",
    "dotenv": "^10.0.0",
    "husky": "^6.0.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "^4.0.7",
    "yargs": "^16.2.0"
  }
}
